        "N = int(input(\"いくつまで数えますか\"))\n",
        "\n",
        "for i in range(1, N+1):\n",
        "  if i%15==0:\n",
        "    print(\"Fizz Buzz\")\n",
        "\n",
        "  elif i%3==0:\n",
        "    print(\"Fizz\")\n",
        "\n",
        "  elif i%5==0:\n",
        "    print(\"Buzz\")\n",
        "\n",
        "  else:\n",
        "    print(i)"
